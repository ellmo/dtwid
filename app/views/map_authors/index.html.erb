<h1>Listing map_authors</h1>

<table>
  <tr>
    <th>Nick</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @map_authors.each do |map_author| %>
  <tr>
    <td><%= map_author.nick %></td>
    <td><%= link_to 'Show', map_author %></td>
    <td><%= link_to 'Edit', edit_map_author_path(map_author) %></td>
    <td><%= link_to 'Destroy', map_author, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Map author', new_map_author_path %>
