<h1>Listing map_episodes</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @map_episodes.each do |map_episode| %>
  <tr>
    <td><%= map_episode.name %></td>
    <td><%= link_to 'Show', map_episode %></td>
    <td><%= link_to 'Edit', edit_map_episode_path(map_episode) %></td>
    <td><%= link_to 'Destroy', map_episode, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Map episode', new_map_episode_path %>
